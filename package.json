{
    "name": "ren-js-example",
    "version": "2.0.0-alpha.0",
    "private": true,
    "homepage": "https://renproject.github.io/gateway-staging",
    "scripts": {
        "start": "react-scripts start",
        "clean": "yarn rimraf ./build",
        "build": "NODE_OPTIONS=--max_old_space_size=4096 REACT_APP_SOURCE_VERSION=$SOURCE_VERSION react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "$npm_execpath run build",
        "deploy": "gh-pages -d build --repo git@github.com:renproject/gateway-staging.git",
        "link:all": "yarn link @renproject/rpc @renproject/interfaces @renproject/utils @renproject/chains-ethereum @renproject/chains-filecoin @renproject/ren",
        "unlink:all": "yarn unlink @renproject/rpc @renproject/interfaces @renproject/utils @renproject/chains-ethereum @renproject/chains-filecoin @renproject/ren"
    },
    "dependencies": {
        "@renproject/chains-bitcoin": "^2.0.0-alpha.11",
        "@renproject/chains-ethereum": "^2.0.0-alpha.11",
        "@renproject/chains-filecoin": "^2.0.0-alpha.11",
        "@renproject/interfaces": "^2.0.0-alpha.11",
        "@renproject/react-components": "^2.0.0-alpha.9",
        "@renproject/ren": "^2.0.0-alpha.11",
        "@renproject/rpc": "^2.0.0-alpha.11",
        "@renproject/utils": "^2.0.0-alpha.11",
        "@types/classnames": "^2.2.10",
        "@types/node": "^13.13.4",
        "@types/qs": "^6.9.1",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@types/react-router-dom": "^5.1.4",
        "bignumber.js": "^9.0.0",
        "classnames": "^2.2.6",
        "immutable": "^4.0.0-rc.12",
        "qs": "^6.9.3",
        "react": "16.12.0",
        "react-copy-to-clipboard": "^5.0.2",
        "react-dom": "16.12.0",
        "react-router-dom": "^5.1.2",
        "web3": "^2.0.0-alpha.1"
    },
    "devDependencies": {
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@types/react-router": "^5.1.8",
        "@types/react-router-dom": "^5.1.5",
        "jest": "^24.9.0",
        "node-sass": "^4.14.1",
        "react": "^16.13.1",
        "react-scripts": "^3.4.3",
        "typescript": "^4.0.2"
    },
    "resolutions": {},
